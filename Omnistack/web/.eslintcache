r\n\r\nimport Sidebar from \"../components/Sidebar\";\r\nimport mapIcon from \"../utilis/mapIcon\";\r\nimport api from \"../services/api\";\r\n\r\ninterface Orphanage {\r\n  latitude: number;\r\n  longitude: number;\r\n  name: string;\r\n  about: string;\r\n  instructions: string;\r\n  opening_hours: string;\r\n  wekeends_open: string;\r\n  images: Array<{ \r\n    id: number;\r\n    url: string;\r\n  }>;\r\n}\r\ninterface OrphanageParams {\r\n  id: string;\r\n}\r\n\r\n\r\nexport default function Orphanage() {\r\n\r\n  const params = useParams<OrphanageParams>();\r\n\r\n  const [orphanage, setOrphanage] = useState<Orphanage>();\r\n\r\n  const [activeimageindex, setActiveimageindex] = useState(0);\r\n\r\n  useEffect(()=> {\r\n    api.get(`orphanages/${params.id}`).then(response => {\r\n        setOrphanage(response.data);\r\n    });\r\n  }, [params.id]);\r\n\r\n  if (!orphanage) {\r\n    return<p>Carregando ...</p>;\r\n  }\r\n\r\n\r\n  return (\r\n    <div id=\"page-orphanage\">\r\n     <Sidebar />\r\n\r\n      <main>\r\n        <div className=\"orphanage-details\">\r\n          <img src={orphanage.images[activeimageindex].url} alt={orphanage.name} />\r\n\r\n          <div className=\"images\">\r\n          {orphanage.images.map((image, index)=> {\r\n              return (\r\n                <button \r\n                key={image.id} \r\n                className={activeimageindex === index ? 'active' : '' }\r\n                type=\"button\"\r\n                onClick={()=>{\r\n                  setActiveimageindex(index);\r\n                }}>\r\n                  <img src={image.url} alt={orphanage.name} />\r\n                </button>\r\n              );\r\n          })}\r\n         \r\n           \r\n          </div>\r\n          \r\n          <div className=\"orphanage-details-content\">\r\n            <h1>{orphanage.name}</h1>\r\n            <p>{orphanage.about}</p>\r\n\r\n            <div className=\"map-container\">\r\n              <Map \r\n                center={[orphanage.latitude, orphanage.longitude]} \r\n                zoom={16} \r\n                style={{ width: '100%', height: 280 }}\r\n                dragging={false}\r\n                touchZoom={false}\r\n                zoomControl={false}\r\n                scrollWheelZoom={false}\r\n                doubleClickZoom={false}\r\n              >\r\n                <TileLayer \r\n                  url={`https://api.mapbox.com/styles/v1/mapbox/light-v10/tiles/256/{z}/{x}/{y}@2x?access_token=${process.env.REACT_APP_MAPBOX_TOKEN}`}\r\n                />\r\n                  <Marker interactive={false} icon={mapIcon} position={[orphanage.latitude, orphanage.longitude]} />\r\n              </Map>\r\n\r\n              <footer>\r\n                <a target=\"_blank\" rel=\"noopener noreferrer\" href={`https://www.google.com/maps/dir/?api=1&destination=${orphanage.latitude},${orphanage.longitude}`}>Ver rotas no Google Maps</a>\r\n              </footer>\r\n            </div>\r\n\r\n            <hr />\r\n\r\n            <h2>Instruções para visita</h2>\r\n            <p>{orphanage.instructions}</p>\r\n\r\n            <div className=\"open-details\">\r\n              <div className=\"hour\">\r\n                <FiClock size={32} color=\"#15B6D6\" />\r\n                Segunda à Sexta <br />\r\n                8h às 18h\r\n              </div>\r\n           {orphanage.wekeends_open ? (\r\n            <div className=\"open-on-weekends\">\r\n              <FiInfo size={32} color=\"#39CC83\" />\r\n              Atendemos <br />\r\n              Fim de semana \r\n            </div>\r\n           ) : ( \r\n            <div className=\"open-on-weekends dont-open\">\r\n              <FiInfo size={32} color=\"#FF669D\" />\r\n              Não atendemos  <br />\r\n              fim de semana \r\n            </div>\r\n           ) }\r\n           </div>\r\n\r\n            {/*\r\n            <button type=\"button\" className=\"contact-button\">\r\n              <FaWhatsapp size={20} color=\"#FFF\" />\r\n              Entrar em contato\r\n            </button>\r\n            */}      \r\n          </div>\r\n        </div>\r\n      </main>\r\n    </div>\r\n  );\r\n}","C:\\Users\\gabriela.candido\\Documents\\GitHub\\Omnistack\\web\\src\\utilis\\mapIcon.ts",[],"C:\\Users\\gabriela.candido\\Documents\\GitHub\\Omnistack\\web\\src\\services\\api.ts",[],"C:\\Users\\gabriela.candido\\Documents\\GitHub\\Omnistack\\web\\src\\components\\Sidebar.tsx",[],{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","severity":1,"message":"52","line":31,"column":25,"nodeType":"53","messageId":"54","endLine":31,"endColumn":34},"no-native-reassign",["55"],"no-negated-in-lhs",["56"],"@typescript-eslint/no-redeclare","'Orphanage' is already defined.","Identifier","redeclared","no-global-assign","no-unsafe-negation"]